{
	"$schema": "http://json.schemastore.org/template",
	"author": "Andreas Müller",
	"classifications": [ "Templating" ],
	"identity": "Amusoft.Templates.TemplateFile.Json",
	"name": "dotnet new template structure",
	"shortName": "dotnet-template",
	"tags": {
		"type": "item"
	},
	"sourceName": "MyTemplate",
	"defaultName": ".template.config",
	"symbols": {
		"author": {
			"type": "parameter",
			"description": "Author of the template",
			"isRequired": true,
			"replaces": "MyAuthor"
		},
		"type": {
			"type": "parameter",
			"description": "Type of the template. Usually project/item",
			"defaultValue": "project",
			"replaces": "MyType"
		}
	}
}